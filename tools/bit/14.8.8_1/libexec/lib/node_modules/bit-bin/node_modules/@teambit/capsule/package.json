{
  "name": "@teambit/capsule",
  "version": "0.0.12",
  "repository": "teambit/capsule",
  "main": "dist/index.js",
  "scripts": {
    "precommit": "lint-staged",
    "test": "mocha --require ts-node/register/transpile-only --require source-map-support/register --recursive src/**/*.spec.ts src/**/**/*.spec.ts",
    "build": "tsc -d",
    "watch": "tsc -d -w",
    "clean": "rm -rf ./dist",
    "start": "node dist/index.js",
    "prettier": "prettier --write 'src/**/*.{ts,}' --write",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/chai": "^4.2.7",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.1.0",
    "@types/sinon": "^7.5.1",
    "chai": "^4.2.0",
    "mocha": "^6.2.2",
    "prettier": "1.19.1",
    "sinon": "^8.0.1",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.4"
  },
  "author": {
    "name": "Team Bit",
    "email": "teambit@bit.dev",
    "url": "https://bit.dev"
  },
  "license": "ISC",
  "dependencies": {
    "@teambit/any-fs": "0.0.5",
    "lodash": "^4.17.15",
    "p-limit": "^2.2.1"
  },
  "lint-staged": {
    "*.{ts,js,json,md,gql}": [
      "prettier --write",
      "git add"
    ]
  }
}
